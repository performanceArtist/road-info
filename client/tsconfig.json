{
    "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./@types"],
    "types": [
      "node",
      "webpack-env"
    ],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
    },
    "exclude": [
      "./node_modules/**/*"
    ]
}
